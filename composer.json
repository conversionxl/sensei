{
	"name": "plugins/sensei-lms",
	"description": "A learning management plugin for WordPress, which provides the smoothest platform for helping you teach anything.",
	"require-dev": {
		"php": "^5.4 || ^7",
		"apigen/apigen": "*",
		"phpunit/phpunit": "^6.5",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
		"phpcompatibility/phpcompatibility-wp": "^2.0",
		"squizlabs/php_codesniffer": "^3.3",
		"wp-coding-standards/wpcs": "^2.2",
		"sirbrillig/phpcs-variable-analysis": "^2.6",
		"sirbrillig/phpcs-no-get-current-user": "^1.0"
	},
	"prefer-stable": true,
	"scripts": {
		"deploy": "dep deploy --hosts=conversionxl.com -o application=$(composer config name)",
		"rollback": "dep rollback --hosts=conversionxl.com -o application=$(composer config name)"
	}
}
